use blog;
DROP TABLE NEWS;
CREATE TABLE NEWS (
	ID  INTEGER AUTO_INCREMENT PRIMARY KEY,
	PUBLISHER VARCHAR(100) DEFAULT NULL,
	journalist VARCHAR(100) DEFAULT NULL,
	TITLE VARCHAR(255) DEFAULT NULL,
	REG_DT DATE DEFAULT NULL	
);

INSERT INTO NEWS( PUBLISHER, journalist, TITLE, REG_DT) VALUES('장우승', '장우승', '타이틀1', SYSDATE());
INSERT INTO NEWS( PUBLISHER, journalist, TITLE, REG_DT) VALUES('한정숙', '한정숙', '타이틀2', SYSDATE());


COMMIT;
